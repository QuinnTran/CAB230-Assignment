                                    }
                                }
                            }
                        }
                    },
                    "400":{
                        "description":"Bad Request",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "Error":{
                                    "type":"string",
                                    "enum":["true","false"]
                                },
                                "Message":{
                                    "type":"string",
                                    "description":["oops! It looks like that user already exists :("]
                                },
                                "Offences":{
                                    "type":"array",
                                    "items":{
                                        "type":"object",
                                        "properties":{
                                            "username":{
                                                "type":"string"
                                            },
                                            "password":{
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/search":{ 
            "get":{
                    "tags":["Search"],
                    "summary":"Search for Offence data.",
                    "description":"Search for Offence data",
                    "operationId":"listSearch",
                    "produces":["application/json"],
                    "parameters":[{
                        "name":"column",
                        "in":"path",
                        "description":"Search for Offence data.",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "example":"homicidemurder"
                        }
                    }],
                    "responses":{
                        "200":{
                            "description":"successful operation",
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "Error":{
                                        "type":"string",
                                        "enum":["true","false"]
                                    },
                                    "Message":{
                                        "type":"string",
                                        "enum":["Success","Failure"]
                                    },
                                    "Offences":{
                                        "type":"array",
                                        "items":{
                                            "type":"object",
                                            "properties":{
                                                "Offence":{
                                                    "type":"string"
                                                },
                                                "Area":{
                                                    "type":"string"
                                                },
                                                "Age":{
                                                    "type":"string"
                                                },
                                                "Gender":{
                                                    "type":"string"
                                                },
                                                "Year":{
                                                    "type":"integer"
                                                },
                                                "Month":{
                                                    "type":"integer"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
