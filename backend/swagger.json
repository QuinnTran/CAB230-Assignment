        },
        "/genders": {
            "get": {
                "tags": [
                    "Helpers"
                ],
                "summary": "List of genders",
                "description": "Full listing of genders",
                "operationId": "listGenders",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "produces": [
                            "application/json"
                        ],
                        "schema": {
                            "$ref": "#/definitions/GEN200"
                        }
                    }
                }
            }
        },
        "/years": {
            "get": {
                "tags": [
                    "Helpers"
                ],
                "summary": "List of years",
                "description": "Full listing of years",
                "operationId": "listYears",
                "produces": [
                    "application/json"
                ],
                "parameters": {},
